@model IEnumerable<dynamic>

<h2>Popular Books</h2>
<table class="table">
    <thead>
        <tr>
            <th>Book Title</th>
            <th>Borrow Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Book.Title</td>
                <td>@item.Count</td>
            </tr>
        }
    </tbody>
</table>
